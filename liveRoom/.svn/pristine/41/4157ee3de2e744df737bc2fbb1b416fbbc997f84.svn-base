package com.liveRoom.bean;

import java.util.Date;

/**
 * 视频评论实体类
 * @author d
 *
 */
public class VideoComments {
	private Integer videocomments_id;//评论id
	private Integer videocomments_userid;//评论人id
	private String videocomments_content;//评论内容
	private Date videocomments_date;//评论时间
	private Integer videocomments_videoid;//视频id
	private Integer videocomments_replyid;//回复id
	
	public Integer getVideocomments_id() {
		return videocomments_id;
	}
	
	public void setVideocomments_id(Integer videocomments_id) {
		this.videocomments_id = videocomments_id;
	}
	
	public Integer getVideocomments_userid() {
		return videocomments_userid;
	}
	
	public void setVideocomments_userid(Integer videocomments_userid) {
		this.videocomments_userid = videocomments_userid;
	}
	
	public String getVideocomments_content() {
		return videocomments_content;
	}
	
	public void setVideocomments_content(String videocomments_content) {
		this.videocomments_content = videocomments_content;
	}
	
	public Date getVideocomments_date() {
		return videocomments_date;
	}
	
	public void setVideocomments_date(Date videocomments_date) {
		this.videocomments_date = videocomments_date;
	}
	
	public Integer getVideocomments_videoid() {
		return videocomments_videoid;
	}
	
	public void setVideocomments_videoid(Integer videocomments_videoid) {
		this.videocomments_videoid = videocomments_videoid;
	}
	
	public Integer getVideocomments_replyid() {
		return videocomments_replyid;
	}
	
	public void setVideocomments_replyid(Integer videocomments_replyid) {
		this.videocomments_replyid = videocomments_replyid;
	}
	
}
